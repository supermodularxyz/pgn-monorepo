{
  "name": "@pgn/react",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org supermodular --project pgn ./dist && sentry-cli sourcemaps upload --org supermodular --project pgn ./dist",
    "tsup": "tsup && npm run sentry:sourcemaps"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-istanbul": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jsdom": "^22.1.0",
    "tailwindcss": "3.3.2",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@conduitxyz/sdk": "^0.0.3",
    "@eth-optimism/sdk": "^3.1.0",
    "@ethersproject/providers": "^5.7.2",
    "@hookform/resolvers": "^3.1.1",
    "@rainbow-me/rainbowkit": "^0.12.15",
    "@sentry/react": "^7.61.0",
    "@tanstack/query-sync-storage-persister": "^4.32.0",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-persist-client": "^4.32.0",
    "@wagmi/chains": "^1.6.0",
    "@wagmi/core": "0.10.16",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "ethers": "^5.7.2",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "tailwind-variants": "^0.1.11",
    "wagmi": "^0.12.16",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
